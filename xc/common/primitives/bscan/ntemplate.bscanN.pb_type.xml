      <direct name="TDO" input="BSCAN{N}.TDO" output="BSCAN_VPR.TDO"/>
    </interconnect>
    <metadata>
      <meta name="fasm_features">
        IN_USE
      </meta>
    </metadata>
  </mode>
</pb_type>
